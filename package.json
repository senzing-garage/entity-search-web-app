    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-in-memory-web-api": "^0.11.0",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "concurrently": "^6.4.0",
    "fs-extra": "^9.0.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.9",
    "karma-brief-reporter": "^0.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "sass-loader": "^6.0.5",
    "scss-bundle": "^3.1.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.6",
    "worker-plugin": "^5.0.1"
  }
