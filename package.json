    "@senzing/rest-api-client-ng": "file:../rest-api-client-ng/dist/@senzing/rest-api-client-ng/senzing-rest-api-client-ng-2.4.0.tgz",
    "@senzing/sdk-components-ng": "file:../sdk-components-ng/dist/@senzing/sdk-components-ng/senzing-sdk-components-ng-2.3.0.tgz",
    "@senzing/sdk-graph-components": "file:../sdk-graph-components/dist/@senzing/sdk-graph-components/senzing-sdk-graph-components-2.5.0.tgz",
    "@types/d3": "^5.7.1",
    "body-parser": "^1.19.0",
    "classlist.js": "^1.1.20150312",
    "cors": "^2.8.5",
    "d3": "^5.9.1",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^6.1.0",
    "hammerjs": "^2.0.8",
    "helmet-csp": "^2.9.5",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^0.19.1",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.3.1",
    "ngx-webstorage-service": "^4.1.0",
    "pug": "^3.0.0",
    "rxjs": "~6.5.5",
    "sanitize-filename": "^1.6.3",
    "serve-static": "^1.13.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "wait-on": "^3.2.0",
    "web-animations-js": "^2.3.2",
    "winston": "^3.2.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.15",
    "@angular-devkit/schematics": "^11.2.15",
    "@angular/cli": "~11.2.15",
