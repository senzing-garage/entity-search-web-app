{
  "/api/*": {
    "target": "http://americium.local:8250",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/admin/auth/jwt/*": {
    "target": "http://localhost:8200/jwt/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/admin/auth/jwt": "/admin"
    }
  },
  "/admin/auth/sso/*": {
    "target": "http://localhost:8200/sso/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/admin/auth/sso": "/admin"
    }
  },
  "/auth/jwt/*": {
    "target": "http://localhost:8200/jwt/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/auth/jwt": ""
    }
  },
  "/auth/sso/*": {
    "target": "http://localhost:8200/sso/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/auth/sso": ""
    }
  },
  "/config/auth": {
    "target": "http://localhost:8200/conf/auth/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/config/auth": ""
    }
  }
}
