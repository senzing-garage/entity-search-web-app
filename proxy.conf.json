{
  "/api/*": {
    "target": "http://localhost:8250",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/admin/auth/jwt/*": {
    "target": "http://localhost:8080/jwt/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/admin/auth/jwt": "/admin"
    }
  },
  "/admin/auth/sso/*": {
    "target": "http://localhost:8080/sso/",
